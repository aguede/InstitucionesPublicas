			<artifactId>mockito-junit-jupiter</artifactId>
			<scope>test</scope>
		</dependency>
	</dependencies>
	<modules>
		<module>instituciones-boot</module>
		<module>instituciones-controller</module>
		<module>instituciones-service</module>
		<module>instituciones-repository</module>
		<module>instituciones-bean</module>
		<module>instituciones-it</module>
